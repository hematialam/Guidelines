(ROOT
  (S
    (NP
      (NP (DT A) (JJ detailed) (NN description))
      (PP (IN of)
        (NP
          (NP
            (NP (DT the) (NNS methods))
            (, ,)
            (NP (NN background))
            (, ,)
            (CC and)
            (NP (NN evidence) (NNS summaries)))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBP support)
                (NP
                  (NP (DT each))
                  (PP (IN of)
                    (NP (DT the) (NNS recommendations))))))))))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN found)
          (PP (IN in)
            (NP
              (NP (DT the) (JJ full) (NN text))
              (PP (IN of)
                (NP (DT this) (NN guideline))))))))
    (. .)))
