        (PP (IN on)
          (NP
            (NP (NN risk) (NN assessment))
            (PP (IN for)
              (NP
                (NP
                  (NP (JJ antimicrobial) (NN resistance))
                  (CC and)
                  (NP (NN evolution)))
                (PP (IN of)
                  (NP (JJ clinical) (NNS responses)))))))))
    (VP (VBZ is)
      (VP (VBN offered)
        (PRN (-LRB- -LRB-)
          (NP (NN Figure) (CD 1))
          (-RRB- -RRB-))))
    (. .)))
