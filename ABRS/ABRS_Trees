(ROOT
  (S
    (NP
      (NP (JJ Evidence-based) (NNS guidelines))
      (PP (IN for)
        (NP
          (NP (DT the) (NN diagnosis))
          (CC and)
          (NP
            (NP (JJ initial) (NN management))
            (PP (IN of)
              (S
                (VP (VBN suspected)
                  (NP
                    (NP (JJ acute) (JJ bacterial) (NN rhinosinusitis))
                    (PP (IN in)
                      (NP
                        (NP (NNS adults))
                        (CC and)
                        (NP (NNS children))))))))))))
    (VP (VBD were)
      (VP (VBN prepared)
        (PP (IN by)
          (NP
            (NP (DT a) (JJ multidisciplinary) (NN expert) (NN panel))
            (PP (IN of)
              (NP
                (NP (DT the) (JJ Infectious) (NNS Diseases))
                (NP
                  (NP (NNP Society))
                  (PP (IN of)
                    (NP (NNP America)))
                  (VP (VBG comprising)
                    (NP
                      (NP (NNS clinicians)
                        (CC and)
                        (NNS investigators))
                      (NP (VBG representing)
                        (NP (JJ internal) (NN medicine))
                        (, ,)
                        (NP (NNS pediatrics))
                        (, ,)
                        (NP
                          (NP (NN emergency) (NN medicine))
                          (, ,)
                          (NP (NN otolaryngology))
                          (, ,)
                          (NP
                            (NP (JJ public) (NN health))
                            (, ,)
                            (NP (NN epidemiology))
                            (, ,)
                            (CC and)
                            (NP
                              (ADJP (JJ adult)
                                (CC and)
                                (JJ pediatric))
                              (JJ infectious) (NN disease) (NNS specialties))))))))))))))
    (. .)))
-> prepared/VBN (root)
  -> guidelines/NNS (nsubjpass)
    -> Evidence-based/JJ (amod)
    -> diagnosis/NN (prep_for)
      -> the/DT (det)
      -> management/NN (conj_and)
        -> initial/JJ (amod)
        -> suspected/VBN (prepc_of)
          -> rhinosinusitis/NN (dobj)
            -> acute/JJ (amod)
            -> bacterial/JJ (amod)
            -> adults/NNS (prep_in)
              -> children/NNS (conj_and)
            -> children/NNS (prep_in)
    -> management/NN (prep_for)
  -> were/VBD (auxpass)
  -> panel/NN (agent)
    -> a/DT (det)
    -> multidisciplinary/JJ (amod)
    -> expert/NN (nn)
    -> Diseases/NNS (prep_of)
      -> the/DT (det)
      -> Infectious/JJ (amod)
      -> Society/NNP (dep)
        -> America/NNP (prep_of)
        -> comprising/VBG (vmod)
          -> clinicians/NNS (dobj)
            -> investigators/NNS (conj_and)
            -> medicine/NN (dep)
              -> representing/VBG (amod)
              -> internal/JJ (amod)
              -> pediatrics/NNS (appos)
              -> medicine/NN (appos)
                -> emergency/NN (nn)
                -> otolaryngology/NN (appos)
                -> health/NN (appos)
                  -> public/JJ (amod)
                  -> epidemiology/NN (conj_and)
                  -> specialties/NNS (conj_and)
                    -> adult/JJ (amod)
                      -> pediatric/JJ (conj_and)
                    -> pediatric/JJ (amod)
                    -> infectious/JJ (amod)
                    -> disease/NN (nn)
                -> epidemiology/NN (appos)
                -> specialties/NNS (appos)
          -> investigators/NNS (dobj)
