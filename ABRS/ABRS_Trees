    (VP (VBG lasting)
      (PP (IN for)
        (NP (CD ≥) (CD 10) (NNS days)))
      (PP (IN without)
        (NP
          (NP (DT any) (NN evidence))
          (PP (IN of)
            (NP
              (NP (JJ clinical) (NN improvement))
              (PRN (-LRB- -LRB-)
                (ADJP
                  (ADJP (JJ strong))
                  (, ,)
                  (ADJP (JJ low-moderate)))
                (-RRB- -RRB-)))))))
    (: ;)))
