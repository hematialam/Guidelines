(ROOT
  (NP
    (NP (JJ Severe) (NN infection))
    (, ,)
    (NP
      (UCP (RB particularly)
        (PP (IN in)
          (NP
            (NP (DT the) (JJ immunocompromised) (NN host))
            (, ,)
            (CC or)
            (NP
              (NP (NNS patients))
              (PP (IN with)
                (NP
                  (NP (JJ multiple) (JJ medical) (NNS problems))
                  (SBAR
                    (WHNP (WDT that))
                    (S
                      (VP (MD may)
                        (VP (VB complicate)
                          (NP (JJ appropriate) (NN dosing)))))))))))
        (CC or))
      (S
        (VP (VB predispose)
          (PP (TO to)
            (NP (JJ unusual) (NNS microorganisms)))
          (, ,)
          (VP (MD should)
            (VP (VB be)
              (VP (VBN referred)
                (PP (TO to)
                  (NP (DT an) (JJ infectious) (NN disease) (NN specialist)))))))))
    (. .)))
-> infection/NN (root)
  -> Severe/JJ (amod)
  -> particularly/RB (appos)
    -> in/IN (dep)
      -> host/NN (pobj)
        -> the/DT (det)
        -> immunocompromised/JJ (amod)
        -> patients/NNS (conj_or)
          -> problems/NNS (prep_with)
            -> multiple/JJ (amod)
            -> medical/JJ (amod)
            -> complicate/VB (rcmod)
              -> that/WDT (nsubj)
              -> may/MD (aux)
              -> dosing/NN (dobj)
                -> appropriate/JJ (amod)
      -> patients/NNS (pobj)
    -> or/CC (cc)
    -> predispose/VB (dep)
      -> microorganisms/NNS (prep_to)
        -> unusual/JJ (amod)
      -> referred/VBN (dep)
        -> should/MD (aux)
        -> be/VB (auxpass)
        -> specialist/NN (prep_to)
          -> an/DT (det)
          -> infectious/JJ (amod)
          -> disease/NN (nn)
########
(ROOT
  (S
    (NP (NNS Patients))
    (NP
      (PP (IN with)
        (NP (JJ recurrent) (NN infection)))
      (VP (CC or)
        (VBN suspected)
        (S
          (VP (TO to)
            (VP (VB have)
              (NP
                (NP (DT an) (JJ underlying) (NN hypersensitivity))
                (CC or)
                (NP (JJ immunologic) (NN disorder))))))))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN referred)
          (PP (TO to)
            (NP (DT an) (NN allergist))))))
    (. .)))
-> referred/VBN (root)
  -> Patients/NNS (nsubjpass)
  -> suspected/VBN (nsubjpass)
    -> infection/NN (prep_with)
      -> recurrent/JJ (amod)
    -> or/CC (cc)
    -> have/VB (xcomp)
      -> to/TO (aux)
      -> hypersensitivity/NN (dobj)
        -> an/DT (det)
        -> underlying/JJ (amod)
        -> disorder/NN (conj_or)
          -> immunologic/JJ (amod)
      -> disorder/NN (dobj)
  -> should/MD (aux)
  -> be/VB (auxpass)
  -> allergist/NN (prep_to)
    -> an/DT (det)
########
(ROOT
  (S
    (NP
      (NP (NNS Patients))
      (PP (IN with)
        (NP
          (NP (JJ rapid) (NN deterioration)
            (CC and)
            (NNS manifestations))
          (ADJP (JJ suggestive)
            (PP (IN of)
              (NP
                (NP
                  (ADJP (JJ orbital)))
                (CC or)
                (NP
                  (ADJP (JJ intracranial)))
                (NP (JJ suppurative) (NNS complications))))))))
    (VP (VBP require)
      (NP
        (NP (JJ urgent) (NN consultation))
        (CC and)
        (NP (DT a) (JJ multidisciplinary) (NN approach))))
    (. .)))
-> require/VBP (root)
  -> Patients/NNS (nsubj)
    -> deterioration/NN (prep_with)
      -> rapid/JJ (amod)
      -> manifestations/NNS (conj_and)
      -> suggestive/JJ (amod)
        -> orbital/JJ (prep_of)
          -> intracranial/JJ (conj_or)
          -> complications/NNS (dep)
            -> suppurative/JJ (amod)
        -> intracranial/JJ (prep_of)
    -> manifestations/NNS (prep_with)
  -> consultation/NN (dobj)
    -> urgent/JJ (amod)
    -> approach/NN (conj_and)
      -> a/DT (det)
      -> multidisciplinary/JJ (amod)
  -> approach/NN (dobj)
