(ROOT
  (NP
    (NP (DT This) (NN guideline) (NNS addresses))
    (NP
      (NP (JJ several) (NNS issues))
      (PP (IN in)
        (NP
          (NP (DT the) (NN management))
          (PP (IN of)
            (NP
              (NP
                (NP (JJ acute) (JJ bacterial) (NN rhinosinusitis))
                (PRN (-LRB- -LRB-)
                  (NP (NN ABRS))
                  (-RRB- -RRB-)))
              (, ,)
              (NP
                (NP (VBG including) (-LRB- -LRB-) (CD 1) (-RRB- -RRB-) (NN inability))
                (PP (IN of)
                  (NP (VBG existing) (JJ clinical) (NNS criteria)))
                (S
                  (VP (TO to)
                    (ADVP (RB accurately))
                    (VP (VB differentiate)
                      (ADJP (JJ bacterial)
                        (PP (IN from)
                          (NP (JJ viral) (JJ acute) (NN rhinosinusitis))))
                      (, ,)
                      (S
                        (VP (VBG leading)
                          (PP (TO to)
                            (NP
                              (ADJP
                                (ADJP (JJ excessive))
                                (CC and)
                                (ADJP (JJ inappropriate)))
                              (JJ antimicrobial) (NN therapy)))))))))
              (: ;)
              (NP
                (LST (-LRB- -LRB-) (LS 2) (-RRB- -RRB-))
                (NP (NNS gaps))
                (PP (IN in)
                  (NP
                    (NP (NN knowledge)
                      (CC and)
                      (NN quality) (NN evidence))
                    (VP (VBG regarding)
                      (NP
                        (NP (JJ empiric) (JJ antimicrobial) (NN therapy))
                        (PP (IN for)
                          (NP (NNS ABRS)))
                        (ADJP (JJ due)
                          (PP (TO to)
                            (NP (JJ imprecise) (NN patient) (NN selection) (NNS criteria)))))))))
              (: ;)
              (NP
                (LST (-LRB- -LRB-) (LS 3) (-RRB- -RRB-))
                (S
                  (S
                    (VP (VBG changing)
                      (NP
                        (NP (NN prevalence)
                          (CC and)
                          (JJ antimicrobial) (NN susceptibility) (NNS profiles))
                        (PP (IN of)
                          (NP
                            (NP (JJ bacterial) (NNS isolates))
                            (VP (VBN associated)
                              (PP (IN with)
                                (NP (NN ABRS)))))))))
                  (: ;)
                  (CC and)
                  (NP
                    (LST (-LRB- -LRB-) (LS 4) (-RRB- -RRB-))
                    (NP (NN impact))
                    (PP (IN of)
                      (NP
                        (NP (DT the) (NN use))
                        (PP (IN of)
                          (NP
                            (NP (VBN conjugated) (NNS vaccines))
                            (PP (IN for)
                              (NP (NN Streptococcus)))))))
                    (FW pneumoniae)
                    (PP (IN on)
                      (NP
                        (NP (DT the) (NN emergence))
                        (PP (IN of)
                          (NP
                            (NP (JJ nonvaccine) (NNS serotypes))
                            (VP (VBN associated)
                              (PP (IN with)
                                (NP (NN ABRS))))))))))))))))
    (. .)))
-> addresses/NNS (root)
  -> This/DT (det)
  -> guideline/NN (nn)
  -> issues/NNS (dep)
    -> several/JJ (amod)
    -> management/NN (prep_in)
      -> the/DT (det)
      -> rhinosinusitis/NN (prep_of)
        -> acute/JJ (amod)
        -> bacterial/JJ (amod)
        -> ABRS/NN (appos)
        -> inability/NN (appos)
          -> including/VBG (amod)
          -> 1/CD (num)
          -> criteria/NNS (prep_of)
            -> existing/VBG (amod)
            -> clinical/JJ (amod)
          -> differentiate/VB (vmod)
            -> to/TO (aux)
            -> accurately/RB (advmod)
            -> bacterial/JJ (acomp)
              -> rhinosinusitis/NN (prep_from)
                -> viral/JJ (amod)
                -> acute/JJ (amod)
            -> leading/VBG (xcomp)
              -> therapy/NN (prep_to)
                -> excessive/JJ (amod)
                  -> inappropriate/JJ (conj_and)
                -> inappropriate/JJ (amod)
                -> antimicrobial/JJ (amod)
        -> gaps/NNS (dep)
          -> 2/LS (dep)
          -> evidence/NN (prep_in)
            -> knowledge/NN (nn)
              -> quality/NN (conj_and)
            -> quality/NN (nn)
            -> regarding/VBG (vmod)
              -> therapy/NN (dobj)
                -> empiric/JJ (amod)
                -> antimicrobial/JJ (amod)
                -> ABRS/NNS (prep_for)
                -> criteria/NNS (prep_due_to)
                  -> imprecise/JJ (amod)
                  -> patient/NN (nn)
                  -> selection/NN (nn)
        -> 3/LS (dep)
          -> changing/VBG (dep)
            -> profiles/NNS (dobj)
              -> prevalence/NN (nn)
                -> antimicrobial/JJ (conj_and)
              -> antimicrobial/JJ (nn)
              -> susceptibility/NN (nn)
              -> isolates/NNS (prep_of)
                -> bacterial/JJ (amod)
                -> associated/VBN (vmod)
                  -> ABRS/NN (prep_with)
            -> impact/NN (conj_and)
              -> 4/LS (dep)
              -> use/NN (prep_of)
                -> the/DT (det)
                -> vaccines/NNS (prep_of)
                  -> conjugated/VBN (amod)
                  -> Streptococcus/NN (prep_for)
              -> pneumoniae/FW (dep)
              -> emergence/NN (prep_on)
                -> the/DT (det)
                -> serotypes/NNS (prep_of)
                  -> nonvaccine/JJ (amod)
                  -> associated/VBN (vmod)
                    -> ABRS/NN (prep_with)
          -> impact/NN (dep)
