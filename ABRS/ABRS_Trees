(ROOT
  (NP
    (NP
      (NP (NN Percentage))
      (PP (IN of)
        (NP
          (NP (NNS patients))
          (VP (VBN treated)
            (PP (IN for)
              (NP
                (NP (NN sinusitis))
                (SBAR
                  (WHNP (WP who))
                  (S
                    (VP (VBD met)
                      (NP
                        (NP (DT the) (NNS criteria))
                        (PP (IN for)
                          (NP (NN therapy))))))))))))
      (PRN (-LRB- -LRB-)
        (PP (VBN based)
          (PP (IN on)
            (NP (NN question) (NN I.))))
        (-RRB- -RRB-)))
    (. .)))
-> Percentage/NN (root)
  -> patients/NNS (prep_of)
    -> treated/VBN (vmod)
      -> sinusitis/NN (prep_for)
        -> met/VBD (rcmod)
          -> who/WP (nsubj)
          -> criteria/NNS (dobj)
            -> the/DT (det)
            -> therapy/NN (prep_for)
  -> on/IN (prepc_based_on)
  -> I./NN (pobj)
    -> question/NN (nn)
