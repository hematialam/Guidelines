(ROOT
  (S
    (NP (DT The) (NN panel))
    (VP (VBD followed)
      (NP
        (NP (DT a) (NN process))
        (VP (VBN used)
          (PP (IN in)
            (NP
              (NP (DT the) (NN development))
              (PP (IN of)
                (NP
                  (NP (JJ other) (JJ Infectious) (NNS Diseases))
                  (SBAR
                    (S
                      (NP
                        (NP
                          (NP (NNP Society))
                          (PP (IN of)
                            (NP (NNP America))))
                        (PRN (-LRB- -LRB-)
                          (NP (NNP IDSA))
                          (-RRB- -RRB-)))
                      (VP (NNS guidelines)
                        (SBAR
                          (S
                            (NP (WDT that))
                            (VP (VBZ includes)
                              (NP
                                (NP (DT a) (JJ systematic) (NN weighting))
                                (PP (IN of)
                                  (NP
                                    (NP (DT the) (NN strength))
                                    (PP (IN of)
                                      (NP
                                        (NP
                                          (NP (NN recommendation))
                                          (PRN (-LRB- -LRB-)
                                            (FRAG
                                              (ADVP (FW eg))
                                              (, ,)
                                              (SQ (`` ``)
                                                (ADJP (JJ high) (, ,) (JJ moderate)
                                                  (PRN (, ,) (JJ low)
                                                    (PRN (, ,)
                                                      (ADJP (RB very) (JJ low))
                                                      ('' ''))))))
                                            (-RRB- -RRB-)))
                                        (CC and)
                                        (NP
                                          (NP (NN quality))
                                          (PP (IN of)
                                            (NP
                                              (NP (NN evidence))
                                              (S (-LRB- -LRB-)
                                                (ADVP (FW eg))
                                                (, ,) (`` ``)
                                                (ADJP (JJ strong) (, ,) (JJ weak) ('' '')))))))))))))))))))))))
      (-RRB- -RRB-)
      (S
        (VP (VBG using)
          (NP
            (NP
              (NP
                (NP
                  (NP (DT the) (NN GRADE))
                  (PRN (-LRB- -LRB-)
                    (NP
                      (NP
                        (NP (NN Grading))
                        (PP (IN of)
                          (NP
                            (NP (NNS Recommendations))
                            (NP (NNP Assessment)))))
                      (, ,)
                      (NP
                        (NP (NNP Development))
                        (CC and)
                        (NP (NN Evaluation))))
                    (-RRB- -RRB-)))
                (NN system))
              (PRN (-LRB- -LSB-)
                (NP
                  (NP (CD 1))
                  (: --)
                  (NP (CD 6)))
                (-RRB- -RSB-)))
            (PRN (-LRB- -LRB-) (NNP Table)
              (LST (LS 1))
              (-RRB- -RRB-))))))
    (. .)))
-> followed/VBD (root)
  -> panel/NN (nsubj)
    -> The/DT (det)
  -> process/NN (dobj)
    -> a/DT (det)
    -> used/VBN (vmod)
      -> development/NN (prep_in)
        -> the/DT (det)
        -> Diseases/NNS (prep_of)
          -> other/JJ (amod)
          -> Infectious/JJ (amod)
          -> guidelines/NNS (rcmod)
            -> Society/NNP (nsubj)
              -> America/NNP (prep_of)
              -> IDSA/NNP (appos)
            -> includes/VBZ (ccomp)
              -> that/WDT (nsubj)
              -> weighting/NN (dobj)
                -> a/DT (det)
                -> systematic/JJ (amod)
                -> strength/NN (prep_of)
                  -> the/DT (det)
                  -> recommendation/NN (prep_of)
                    -> eg/FW (dep)
                      -> moderate/JJ (dep)
                        -> high/JJ (amod)
                        -> low/JJ (dep)
                          -> low/JJ (dep)
                            -> very/RB (advmod)
                    -> quality/NN (conj_and)
                      -> evidence/NN (prep_of)
                        -> weak/JJ (dep)
                          -> eg/FW (advmod)
                          -> strong/JJ (amod)
                  -> quality/NN (prep_of)
  -> using/VBG (xcomp)
    -> system/NN (dobj)
      -> GRADE/NN (nn)
        -> the/DT (det)
        -> Grading/NN (dep)
          -> Recommendations/NNS (prep_of)
            -> Assessment/NNP (appos)
          -> Development/NNP (appos)
            -> Evaluation/NN (conj_and)
          -> Evaluation/NN (appos)
      -> 1/CD (dep)
        -> -LSB-/-LRB- (punct)
        -> 6/CD (dep)
        -> -RSB-/-RRB- (punct)
      -> Table/NNP (dep)
        -> 1/LS (dep)
