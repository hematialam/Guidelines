      (NP
        (NP (PRP$ their) (NN reliability))
        (PP (IN in)
          (NP (NNS children))))
      (VP (VBZ has) (RB not)
        (VP (VBN been)
          (VP (VBN established)
            (PRN (-LRB- -LRB-)
              (ADJP (JJ weak) (, ,) (JJ moderate))
              (-RRB- -RRB-))))))
    (. .)))
