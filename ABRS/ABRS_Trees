(ROOT
  (S
    (S
      (VP (VB Delay)
        (PP (IN in)
          (NP (JJ appropriate) (NN referral)))
        (PP (TO to)
          (NP (NNS specialists)))))
    (VP (MD may)
      (VP (VB prolong)
        (S
          (NP
            (NP (NN illness))
            (, ,)
            (VP (VBP result)
              (PP (IN in)
                (NP (JJ chronic) (NN disease))))
            (, ,)
            (CC and)
            (ADVP (RB occasionally)))
          (VP (VB lead)
            (PP (TO to)
              (NP (JJ catastrophic) (NNS consequences)))
            (SBAR (IN if)
              (S
                (NP (JJ life-threatening) (NNS complications))
                (VP (VBP are) (RB not)
                  (VP (VBN recognized)))))))))
    (. .)))
-> prolong/VB (root)
  -> Delay/VB (csubj)
    -> referral/NN (prep_in)
      -> appropriate/JJ (amod)
    -> specialists/NNS (prep_to)
  -> may/MD (aux)
  -> lead/VB (ccomp)
    -> illness/NN (nsubj)
      -> result/VBP (conj_and)
        -> disease/NN (prep_in)
          -> chronic/JJ (amod)
      -> occasionally/RB (advmod)
    -> result/VBP (nsubj)
    -> consequences/NNS (prep_to)
      -> catastrophic/JJ (amod)
    -> recognized/VBN (advcl)
      -> if/IN (mark)
      -> complications/NNS (nsubjpass)
        -> life-threatening/JJ (amod)
      -> are/VBP (auxpass)
      -> not/RB (neg)
########
(ROOT
  (S
    (NP (JJ Unnecessary) (NN referral))
