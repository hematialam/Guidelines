          (WHNP (WP who))
          (S
            (VP (VBP are)
              (NP
                (NP (NN meeting) (NN treatment) (NNS goals))
                (-LRB- -LRB-)
                (CC and)
                (SBAR
                  (WHNP (WP who))
                  (S
                    (VP (VBP have)
                      (NP (JJ stable) (JJ glycemic) (NN control)))))))))))
    (-RRB- -RRB-) (. .)))
