(ROOT
  (S
    (S
      (NP
        (NP (NN •) (NN Metformin) (NN therapy))
        (PP (IN for)
          (NP
            (NP (NN prevention))
            (PP (IN of)
              (NP (NN type) (CD 2) (NN diabetes))))))
      (VP (MD may)
        (VP (VB be)
          (VP (VBN considered)
            (PP (IN in)
              (NP
                (NP (DT those))
                (PP (IN with)
                  (NP
                    (NP
                      (NP (NN IGT))
                      (PRN (-LRB- -LRB-)
                        (NP (NN A))
                        (-RRB- -RRB-)))
                    (, ,)
                    (NP
                      (NP (NN IFG))
                      (PRN (-LRB- -LRB-)
                        (NP (NN E))
                        (-RRB- -RRB-)))
                    (, ,)
                    (CC or)
                    (NP (DT an)
                      (NP (NN A1C) (CD 5.7)))))))))))
    (: --)
    (NP
      (NP
        (NP (CD 6.4) (NN %))
        (PRN (-LRB- -LRB-)
          (NP (NN E))
          (-RRB- -RRB-)))
      (, ,)
      (PP
        (ADVP (RB especially))
        (IN for)
        (NP
          (NP (DT those))
          (PP (IN with)
            (NP
              (NP (NN BMI))
              (NP
                (NP
                  (QP (JJR >) (CD 35))
                  (NN kg/m2))
                (, ,)
                (NP (JJ aged)
                  (QP (JJR <) (CD 60))
                  (NNS years))
                (, ,)
                (CC and)
                (NP
                  (NP (NNS women))
                  (PP (IN with)
                    (NP (JJ prior) (NN GDM))))))))))
    (. .)))
-> considered/VBN (root)
  -> therapy/NN (nsubjpass)
    -> •/NN (nn)
    -> Metformin/NN (nn)
    -> prevention/NN (prep_for)
      -> diabetes/NN (prep_of)
        -> type/NN (nn)
        -> 2/CD (num)
  -> may/MD (aux)
  -> be/VB (auxpass)
  -> those/DT (prep_in)
    -> IGT/NN (prep_with)
      -> A/NN (appos)
      -> IFG/NN (conj_or)
        -> E/NN (appos)
      -> A1C/NN (conj_or)
        -> an/DT (det)
        -> 5.7/CD (num)
    -> IFG/NN (prep_with)
    -> A1C/NN (prep_with)
  -> %/NN (dep)
    -> 6.4/CD (num)
    -> E/NN (appos)
    -> especially/RB (advmod)
    -> those/DT (prep_for)
      -> BMI/NN (prep_with)
        -> kg/m2/NN (dep)
          -> 35/CD (num)
            -> >/JJR (quantmod)
          -> years/NNS (conj_and)
            -> aged/JJ (amod)
            -> 60/CD (num)
              -> </JJR (quantmod)
          -> women/NNS (conj_and)
            -> GDM/NN (prep_with)
              -> prior/JJ (amod)
