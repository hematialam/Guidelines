(ROOT
  (NP
    (NP
      (NP (NN Prevention/delay))
      (PP (IN of)
        (NP (NN type) (CD 2) (NN diabetes)))
      (NP
        (NP (CD •) (NNS Patients))
        (PP (IN with)
          (NP
            (NP (JJ impaired)
              (NP (NN glucose) (NN tolerance))
              (PRN (-LRB- -LRB-)
                (NP (NN IGT))
                (-RRB- -RRB-)))
            (PRN (-LRB- -LRB-)
              (NP (NN A))
              (-RRB- -RRB-)))))
      (, ,)
      (NP
        (NP (JJ impaired) (NN fasting) (NN glucose) (-LRB- -LRB-) (NN IFG) (-RRB- -RRB-))
        (PRN (-LRB- -LRB-)
          (NP (NN E))
          (-RRB- -RRB-)))
      (, ,)
      (CC or)
      (NP (DT an)
        (NP (NN A1C) (CD 5.7)))
      (: --)
      (NP
        (NP (CD 6.4) (NN %))
        (PRN (-LRB- -LRB-)
          (NP (NN E))
          (-RRB- -RRB-)))
      (SBAR
        (S
          (VP (MD should)
            (VP (VB be)
              (VP (VBN referred)
                (PP (TO to)
                  (NP
                    (NP (DT an) (JJ effective) (JJ ongoing) (NN support) (NN program))
                    (VP
                      (VP (VBG targeting)
                        (NP
                          (NP (NN weight) (NN loss))
                          (PP (IN of)
                            (NP
                              (NP (CD 7) (NN %))
                              (PP (IN of)
                                (NP (NN body) (NN weight)))))))
                      (CC and)
                      (VP (VBG increasing)
                        (NP (JJ physical) (NN activity))
                        (PP (TO to)
                          (NP
                            (NP
                              (NP
                                (QP (IN at) (JJS least) (CD 150))
                                (NN min/week))
                              (PP (IN of)
                                (NP (JJ moderate) (NN activity))))
                            (PP (JJ such) (IN as)
                              (S
                                (VP (VBG walking))))))))))))))))
    (. .)))
-> Prevention/delay/NN (root)
  -> diabetes/NN (prep_of)
    -> type/NN (nn)
    -> 2/CD (num)
  -> Patients/NNS (dep)
    -> •/CD (num)
    -> tolerance/NN (prep_with)
      -> impaired/JJ (amod)
      -> glucose/NN (nn)
      -> IGT/NN (appos)
      -> A/NN (appos)
  -> IFG/NN (conj_or)
    -> impaired/JJ (amod)
    -> fasting/NN (nn)
    -> glucose/NN (nn)
    -> E/NN (appos)
  -> A1C/NN (conj_or)
    -> an/DT (det)
    -> 5.7/CD (num)
  -> %/NN (conj_or)
    -> 6.4/CD (num)
    -> E/NN (appos)
  -> referred/VBN (dep)
    -> should/MD (aux)
    -> be/VB (auxpass)
    -> program/NN (prep_to)
      -> an/DT (det)
      -> effective/JJ (amod)
      -> ongoing/JJ (amod)
      -> support/NN (nn)
      -> targeting/VBG (dep)
        -> loss/NN (dobj)
          -> weight/NN (nn)
          -> %/NN (prep_of)
            -> 7/CD (num)
            -> weight/NN (prep_of)
              -> body/NN (nn)
        -> increasing/VBG (conj_and)
          -> activity/NN (dobj)
            -> physical/JJ (amod)
          -> min/week/NN (prep_to)
            -> 150/CD (num)
              -> at/IN (quantmod)
                -> least/JJS (mwe)
            -> activity/NN (prep_of)
              -> moderate/JJ (amod)
            -> walking/VBG (prepc_such_as)
      -> increasing/VBG (dep)
