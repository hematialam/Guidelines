(ROOT
  (S
    (NP
      (NP (NNP Diabetes) (NNP Care))
      (PP (IN in)
        (NP (DT the) (NNP Hospital))))
    (VP (VBZ has)
      (VP (VBN been)
        (VP (VBN revised)
          (S
            (VP (TO to)
              (VP (VB include)
                (NP (DT a) (NN recommendation)
                  (S
                    (VP (TO to)
                      (VP (VB consider)
                        (S
                          (VP (VBG obtaining)
                            (NP
                              (NP (DT an) (NN A1C))
                              (PP (IN in)
                                (NP
                                  (NP (NNS patients))
                                  (PP (IN with)
                                    (NP (NN risk) (NNS factors)))))
                              (PP (IN for)
                                (NP
                                  (NP (JJ undiagnosed) (NN diabetes))
                                  (SBAR
                                    (WHNP (WP who))
                                    (S
                                      (VP (VBP exhibit)
                                        (NP (NN hyperglycemia))
                                        (PP (IN in)
                                          (NP (DT the) (NN hospital)))))))))))))))))))))
    (. .)))
-> revised/VBN (root)
  -> Care/NNP (nsubjpass)
    -> Diabetes/NNP (nn)
    -> Hospital/NNP (prep_in)
      -> the/DT (det)
  -> has/VBZ (aux)
  -> been/VBN (auxpass)
  -> include/VB (xcomp)
    -> to/TO (aux)
    -> recommendation/NN (dobj)
      -> a/DT (det)
      -> consider/VB (vmod)
        -> to/TO (aux)
        -> obtaining/VBG (xcomp)
          -> A1C/NN (dobj)
            -> an/DT (det)
            -> patients/NNS (prep_in)
              -> factors/NNS (prep_with)
                -> risk/NN (nn)
            -> diabetes/NN (prep_for)
              -> undiagnosed/JJ (amod)
              -> exhibit/VBP (rcmod)
                -> who/WP (nsubj)
                -> hyperglycemia/NN (dobj)
                -> hospital/NN (prep_in)
                  -> the/DT (det)
