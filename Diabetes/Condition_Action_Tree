      (VP (VB be)
        (VP
          (UCP (VBN measured)
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (EX there))
                (VP (VBZ is)
                  (NP
                    (NP (DT a) (NN discrepancy))
                    (PP (IN between)
                      (NP
                        (NP (JJ preprandial) (NN blood) (NN glucose) (NNS values))
                        (CC and)
                        (NP (NN A1C) (NNS levels))))))))
            (CC and))
          (S
            (VP (TO to)
              (VP (VB help)
                (S
                  (VP (VB assess)
                    (NP (NN glycemia))
                    (PP (IN in)
                      (NP
                        (NP (DT those))
                        (PP (IN on)
                          (NP (NN basal/bolus) (NNS regimens)))))))))))))
    (. .)))
