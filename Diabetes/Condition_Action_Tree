    (NP
      (NP (NN Prevention/delay))
      (PP (IN of)
        (NP (NN type) (CD 2) (NN diabetes)))
      (NP
        (NP (CD •) (NNS Patients))
        (PP (IN with)
          (NP
            (NP (JJ impaired)
              (NP (NN glucose) (NN tolerance))
              (PRN (-LRB- -LRB-)
                (NP (NN IGT))
                (-RRB- -RRB-)))
            (PRN (-LRB- -LRB-)
              (NP (NN A))
              (-RRB- -RRB-)))))
      (, ,)
      (NP
        (NP (JJ impaired) (NN fasting) (NN glucose) (-LRB- -LRB-) (NN IFG) (-RRB- -RRB-))
        (PRN (-LRB- -LRB-)
          (NP (NN E))
          (-RRB- -RRB-)))
      (, ,)
      (CC or)
      (NP (DT an)
        (NP (NN A1C) (CD 5.7)))
      (: --)
      (NP
        (NP (CD 6.4) (NN %))
        (PRN (-LRB- -LRB-)
          (NP (NN E))
          (-RRB- -RRB-)))
      (SBAR
        (S
          (VP (MD should)
            (VP (VB be)
              (VP (VBN referred)
                (PP (TO to)
                  (NP
                    (NP (DT an) (JJ effective) (JJ ongoing) (NN support) (NN program))
                    (VP
                      (VP (VBG targeting)
                        (NP
                          (NP (NN weight) (NN loss))
                          (PP (IN of)
                            (NP
                              (NP (CD 7) (NN %))
                              (PP (IN of)
                                (NP (NN body) (NN weight)))))))
                      (CC and)
                      (VP (VBG increasing)
                        (NP (JJ physical) (NN activity))
                        (PP (TO to)
                          (NP
                            (NP
                              (NP
                                (QP (IN at) (JJS least) (CD 150))
                                (NN min/week))
                              (PP (IN of)
                                (NP (JJ moderate) (NN activity))))
                            (PP (JJ such) (IN as)
                              (S
                                (VP (VBG walking))))))))))))))))
    (. .)))
