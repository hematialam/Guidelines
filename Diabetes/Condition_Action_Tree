(ROOT
  (S
    (PP (IN In)
      (S
        (VP (VBG selecting)
          (NP (JJ glycemic) (NNS goals)))))
    (, ,)
    (NP
      (NP (DT the) (NNS benefits))
      (PP (IN on)
        (NP
          (NP (JJ long-term) (NN health) (NNS outcomes))
          (PP (IN of)
            (S
              (VP (VBG achieving)
                (NP (DT a) (JJR lower) (NN A1C))))))))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN balanced)
          (PP (IN against)
            (NP
              (NP
                (NP (DT the) (NNS risks))
                (PP (IN of)
                  (NP (NN hypoglycemia))))
              (CC and)
              (NP
                (NP (DT the) (JJ developmental) (NNS burdens))
                (PP (IN of)
                  (NP
                    (NP (JJ intensive) (NNS regimens))
                    (PP (IN in)
                      (NP
                        (NP (NNS children))
                        (CC and)
                        (NP (NN youth))))))))))))
    (. .)))
-> balanced/VBN (root)
  -> selecting/VBG (prepc_in)
    -> goals/NNS (dobj)
      -> glycemic/JJ (amod)
  -> benefits/NNS (nsubjpass)
    -> the/DT (det)
    -> outcomes/NNS (prep_on)
      -> long-term/JJ (amod)
      -> health/NN (nn)
      -> achieving/VBG (prepc_of)
        -> A1C/NN (dobj)
          -> a/DT (det)
          -> lower/JJR (amod)
  -> should/MD (aux)
  -> be/VB (auxpass)
  -> risks/NNS (prep_against)
    -> the/DT (det)
    -> hypoglycemia/NN (prep_of)
    -> burdens/NNS (conj_and)
      -> the/DT (det)
      -> developmental/JJ (amod)
      -> regimens/NNS (prep_of)
        -> intensive/JJ (amod)
        -> children/NNS (prep_in)
          -> youth/NN (conj_and)
        -> youth/NN (prep_in)
  -> burdens/NNS (prep_against)
